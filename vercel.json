{
  "redirects": [
    {
      "source": "http://nextgenlabz.studio/(.*)",
      "destination": "https://www.nextgenlabz.studio/$1",
      "permanent": true
    },
    {
      "source": "https://nextgenlabz.studio/(.*)",
      "destination": "https://www.nextgenlabz.studio/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|svg|webp|ico|xml|txt|json|css|js)",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
